# Docker Environment File Template
# Copy this to .env and fill in your values

# AWS Configuration
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_DEFAULT_REGION=eu-north-1

# EMR Configuration
EMR_CLUSTER_ID=j-YOUR-CLUSTER-ID
EMR_EC2_ROLE=EMR_EC2_DefaultRole
EMR_ROLE=EMR_DefaultRole

# S3 Paths
SCRIPT_BUCKET=your-scripts-bucket-name
BRONZE_PATH=s3://your-bronze-bucket/clickstream/
EMR_LOG_URI=s3://your-logs-bucket/emr-logs/

# Iceberg Configuration
ICEBERG_DB=analytics
